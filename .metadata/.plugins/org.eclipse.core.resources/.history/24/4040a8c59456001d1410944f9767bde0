	private GUIbase() {}
	
	static Font fontLabelSmall = new Font("Tahoma", Font.BOLD, 12);
	static Font fontLabelLarge = new Font("Times New Roman", Font.PLAIN, 14);
	static Font fontButton = new Font("Tahoma", Font.PLAIN, 13);
}
